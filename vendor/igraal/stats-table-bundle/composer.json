{
    "name": "igraal/stats-table-bundle",
    "description": "Symfony bundle to work with stats-table",
    "homepage": "https://github.com/igraal/stats-table-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["PHP", "stats", "statistics", "excel", "csv", "json", "xls"],
    "authors": [
        {
            "name": "iGraal",
            "homepage": "http://www.igraal.com/"
        }
    ],
    "autoload": {
        "psr-4": {"": "src"}
    },
    "suggest": {
        "phpoffice/phpexcel": "Allow to export to Excel file. Add phpoffice/phpexcel *@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpmd/phpmd": "*@dev",
        "squizlabs/php_codesniffer": "2.0.*@dev",
        "symfony/console": "^2.3|^3.0",
        "twig/twig": "^1.0"
    },
    "require": {
        "php": "^5.6|^7.0",
        "igraal/stats-table": "^1.0",
        "sensio/framework-extra-bundle": ">=2.3,<6"
    }
}
