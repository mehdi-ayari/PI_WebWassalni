{
    "name": "shapecode/cron-bundle",
    "description": "This bundle provides scheduled execution of Symfony2 commands",
    "keywords": [
        "cron",
        "cronjob",
        "command",
        "execute",
        "interval",
        "schedule",
        "shapecode",
        "symfony",
        "time",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/cron-bundle",
    "support": {
        "email": "support@shapeocode.de",
        "issues": "https://github.com/shapecode/cron-bundle/issues",
        "source": "https://github.com/shapecode/cron-bundle/releases",
        "wiki": "https://github.com/shapecode/cron-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "https://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/shapecode/cron-bundle/graphs/contributors"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/console": "~2.8|~3.0|~4.0",
        "symfony/http-foundation": "~2.8|~3.0|~4.0",
        "symfony/process": "~2.8|~3.0|~4.0",
        "symfony/stopwatch": "~2.8|~3.0|~4.0",
        "symfony/event-dispatcher": "~2.8|~3.0|~4.0",

        "doctrine/orm": "~2.5",
        "doctrine/common": "~2.6",
        "doctrine/doctrine-bundle": "~1.6",

        "mtdowling/cron-expression": "~1.1"
    },
    "require-dev":{
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^1.0"
    },
    "suggest": {
        "sonata-project/admin-bundle": "to manage cronjobs in the sonata admin panel"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\CronBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\CronBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "3.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
